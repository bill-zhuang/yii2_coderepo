{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Coderepo",
    "keywords": ["yii2", "framework", "coderepo"],
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.11",
        "yiisoft/yii2-bootstrap": "2.0.6",
        "yiisoft/yii2-swiftmailer": "2.1.0",
        "components/jquery-cookie": "1.4.1.2",
        "components/bootstrap-datetimepicker": "2.4.4",
        "abeautifulsite/jquery-minicolors": "2.2.6",
        "bootstrap-select/bootstrap-select": "1.7.2",
        "ckeditor/ckeditor": "4.7.3",
        "miloschuman/yii2-highcharts-widget": "5.0.2",
        "fortawesome/font-awesome": "4.7.0",
        "driftyco/ionicons": "2.0.1",
        "almasaeed2010/adminlte": "2.3.11",
        "afarkas/html5shiv": "3.7.3",
        "rogeriopradoj/respond": "1.4.2",
        "e96/yii2-sentry": "0.2.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "bower-asset/typeahead.js": "0.10.*"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
